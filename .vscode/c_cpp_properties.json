{
  "version": 4,
  "configurations": [
    {
      "name": "Linux",
      "intelliSenseMode": "linux-gcc-x64",
      "includePath": [
        "${workspaceFolder}/include/external/**",
        "${workspaceFolder}/include"
      ],
      "forcedInclude": [
      ],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "compileCommands": "${workspaceFolder}/build/compile_commands.json",
      "browse": {
        "path": [
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    },
    {
      "name": "Win32",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/include/external/**"
      ],
      "forcedInclude": [
      ],
      "cStandard": "c17",
      "cppStandard": "c++20",
      "browse": {
        "path": [
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    },
    {
      "name": "Mac",
      "intelliSenseMode": "clang-x64",
      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/include/external/**"
      ],
      "forcedInclude": [
      ],
      "macFrameworkPath": [
        "/System/Library/Frameworks"
      ],
      "defines": [],
      "compilerPath": "/usr/bin/clang",
      "cStandard": "c17",
      "cppStandard": "c++20",
      "compileCommands": "${workspaceFolder}/build/compile_commands.json",
      "browse": {
        "path": [
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ]
}